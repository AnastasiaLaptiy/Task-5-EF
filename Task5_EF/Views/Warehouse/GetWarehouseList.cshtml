@model IEnumerable<Task5_EF.Models.WarehouseModel>

@{
    ViewBag.Title = "Warehouse List";
}

    <h2>Warehouse List</h2>

<p>
    @Html.ActionLink("Create New", "CreateWarehouse")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditWarehouse", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteWarehouse", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
